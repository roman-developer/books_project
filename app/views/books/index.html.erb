<h1><%= I18n.t('books.title') %></h1>
<%= link_to I18n.t('books.create_book'), new_book_path %>
<ul>
	<% @books.each do|book| %>
		<li><%= book.title %> - <%= link_to 'Show', book_path(book) %> - <%= link_to 'Edit', edit_book_path(book) %> - <%= link_to 'Delete', book_path(book), data: {confirm: I18n.t('general.message.delete')}, method: :delete %></li>
	<% end %>
</ul>